<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.mymap"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk android:minSdkVersion="15" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE"/>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>
    <supports-screens android:smallScreens="true" android:resizeable="true" android:anyDensity="true" android:largeScreens="true" android:normalScreens="true"/>

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" android:name="MapManagerApp">
        <activity
            android:name=".MyMapActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="NearbySearchActivity"></activity>
        <activity android:name="ViewInMap"></activity>
        <activity android:name="DriveRoute"></activity>
        <activity android:name="RouteSearch"></activity>
        <activity android:name="DriveRoute"></activity>
        <activity android:name="DriveRouteMap"></activity>
        <activity android:name="GetAddr"></activity>
        <activity android:name="Walkroute"></activity>
        <activity android:name="WalkRouteMap"></activity>
        <activity android:name="TransitRoute"></activity>
        <activity android:name="TransitRouteMap"></activity>
        <activity android:name="DetailedInfo"></activity>
        <activity android:name="MyFavour"></activity>
        <activity android:name="MyFavourMap"></activity>
        <activity android:name="ChangeCity"></activity>
        <activity android:name="OfflineMap"></activity>
        <activity android:name="Settings"></activity>
    </application>

</manifest>